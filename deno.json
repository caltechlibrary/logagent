{
  "tasks": {
    "check": "deno task logagent.ts",
    "logagent.ts": "deno check logagent.ts ; deno check logagent_test.ts",
    "test": "deno task logagent_test.ts ; deno task readlines_test.ts ; deno task logtok_test.ts; deno task loganalyst_test.ts ",
    "logagent_test.ts": "deno test --allow-read --allow-run logagent_test.ts",
    "loganalyst_test.ts": "deno test --allow-read loganalyst_test.ts",
    "readlines_test.ts": "deno test readlines_test.ts",
    "logtok_test.ts": "deno test logtok_test.ts",
    "build": "deno task gen-code ; deno task compile ; deno task manpage",
  	"compile": "deno task compile_logagent ; deno task compile_loganalyst",
    "compile_logagent": "deno compile --allow-read --allow-write --allow-run --output ./bin/logagent logagent.ts",
    "compile_loganalyst": "deno compile --allow-read --allow-write --output ./bin/loganalyst loganalyst.ts",
    "about.md": "cmt codemeta.json about.md",
    "CITATION.cff": "cmt codemeta.json CITATION.cff",
    "version.ts": "cmt codemeta.json version.ts",
    "gen-code": "deno task about.md ; deno task CITATION.cff ; deno task version.ts",
    "manpage": " deno task logagent.1.md ; deno task loganalyst.1.md",
	  "logagent.1.md": "deno run --allow-read --allow-write --allow-run logagent.ts --help >logagent.1.md",
	  "loganalyst.1.md": "deno run --allow-read --allow-write loganalyst.ts --help >loganalyst.1.md",
    "dist_linux_x86_64": "deno task dist_linux_x86_64_logagent; deno task dist_linux_x86_64_loganalyst",
    "dist_linux_aarch64": "deno task dist_linux_aarch64_logagent; deno task dist_linux_aarch64_loganalyst",
    "dist_macos_x86_64": "deno task dist_macos_x86_64_logagent; deno task dist_macos_x86_64_loganalyst",
    "dist_macos_aarch64": "deno task dist_macos_aarch64_logagent; deno task dist_macos_aarch64_loganalyst",
    "dist_windows_x86_64": "deno task dist_windows_x86_64_logagent; deno task dist_windows_x86_64_loganalyst",
    "dist_windows_aarch64": "deno task dist_windows_aarch64_logagent; deno task dist_windows_aarch64_loganalyst",
    "dist_linux_x86_64_logagent": "deno compile   --allow-read --allow-write --allow-run --output dist/bin/logagent --target x86_64-unknown-linux-gnu logagent.ts",
    "dist_linux_aarch64_logagent": "deno compile  --allow-read --allow-write --allow-run --output dist/bin/logagent --target aarch64-unknown-linux-gnu logagent.ts",
    "dist_macos_x86_64_logagent": "deno compile   --allow-read --allow-write --allow-run --output dist/bin/logagent --target x86_64-apple-darwin logagent.ts",
    "dist_macos_aarch64_logagent": "deno compile  --allow-read --allow-write --allow-run --output dist/bin/logagent --target aarch64-apple-darwin logagent.ts",
    "dist_windows_x86_64_logagent": "deno compile --allow-read --allow-write --allow-run --output dist/bin/logagent.exe --target x86_64-pc-windows-msvc logagent.ts",
    "dist_windows_aarch64_logagent": "deno compile --allow-read --allow-write --allow-run --output dist/bin/logagent.exe --target aarch64-pc-windows-msvc logagent.ts",
    "dist_linux_x86_64_loganalyst": "deno compile   --allow-read --allow-write --allow-run --output dist/bin/loganalyst --target x86_64-unknown-linux-gnu loganalyst.ts",
    "dist_linux_aarch64_loganalyst": "deno compile  --allow-read --allow-write --allow-run --output dist/bin/loganalyst --target aarch64-unknown-linux-gnu loganalyst.ts",
    "dist_macos_x86_64_loganalyst": "deno compile   --allow-read --allow-write --allow-run --output dist/bin/loganalyst --target x86_64-apple-darwin loganalyst.ts",
    "dist_macos_aarch64_loganalyst": "deno compile  --allow-read --allow-write --allow-run --output dist/bin/loganalyst --target aarch64-apple-darwin loganalyst.ts",
    "dist_windows_x86_64_loganalyst": "deno compile --allow-read --allow-write --allow-run --output dist/bin/loganalyst.exe --target x86_64-pc-windows-msvc loganalyst.ts",
    "dist_windows_aarch64_loganalyst": "deno compile --allow-read --allow-write --allow-run --output dist/bin/loganalyst.exe --target aarch64-pc-windows-msvc loganalyst.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.11",
    "@std/cli": "jsr:@std/cli@^1.0.11",
    "@std/yaml": "jsr:@std/yaml@^1.0.5"
  }
}
